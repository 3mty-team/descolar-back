{
    "name": "descolar/descolar-back",
    "description": "API REST for Descolar project",
    "type": "project",
    "authors": [
        {
            "name": "Descolar Team",
            "email": "contact@descolar.fr"
        }
    ],
    "require": {
        "phpstan/phpstan": "^1.10.67",
        "symfony/var-dumper": "^v6.4.6",
        "zircote/swagger-php": "^4.9.0",
        "doctrine/orm": "^3.1.2",
        "symfony/cache": "^v7.0.6",
        "filp/whoops": "2.15.4",
        "firebase/php-jwt": "v6.10.0",
        "phpmailer/phpmailer": "^v6.9.1",
        "cboden/ratchet": "v0.5.x-dev"
    },
    "require-dev": {
        "pestphp/pest": "^v2.34.7"
    },
    "autoload": {
        "psr-4": {
            "Descolar\\": "App/"
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
