{
    "name": "descolar/descolar-back",
    "description": "Projet BackEnd de Descolar",
    "type": "project",
    "require": {
        "phpstan/phpstan": "^1.10",
        "symfony/var-dumper": "^6.3",
        "zircote/swagger-php": "^4.8",
        "doctrine/orm": "^3.0",
        "symfony/cache": "^7.0",
        "filp/whoops": "2.*",
        "firebase/php-jwt": "v6.10.0"
    },
    "require-dev": {
        "pestphp/pest": "^2.24"
    },
    "autoload": {
        "psr-4": {
            "Descolar\\": "App/"
        }
    },
    "authors": [
        {
            "name": "Descolar Team"
        }
    ],
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
